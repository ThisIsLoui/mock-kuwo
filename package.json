    "video.js": "^8.10.0",
    "vite": "^5.2.0"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ],
    "*.{vue,scss,css}": [
      "eslint --fix",
      "stylelint --fix"
