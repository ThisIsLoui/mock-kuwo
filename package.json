    "axios": "^1.6.8",
    "classnames": "^2.5.1",
    "modern-normalize": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-paginate": "^8.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3",
    "react-use-audio-player": "^2.2.0",
    "redux-persist": "^6.0.0",
    "swiper": "^11.1.1"
